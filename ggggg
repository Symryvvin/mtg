enum Language {
	ZH-CN("Chinese Simplified", "zh_CN"),
	ZH_TW("Chinese Traditional", "zh_TW"),
	DE("German", "de"),
	EN("English", "en"),
	FR("French", "fr"),
	IT("Italian", "it"),
	JA("Japanese", "ja"),
	KO("Korean", "ko"),
	PT_BR("Portuguese (Brazil)", "pt_BR"),
	RU("Russian", "ru"),
	ES("Spanish", "es");
}

