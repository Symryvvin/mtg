VALUES ('es', 'Spanish');
INSERT INTO lang (code, description)
VALUES ('pt_br', 'Portuguese (Brazil)');
INSERT INTO lang (code, description)
VALUES ('ja', 'Japanese');
INSERT INTO lang (code, description)
VALUES ('ko', 'Korean');
INSERT INTO lang (code, description)
VALUES ('zh_cn', 'Chinese Simplified');
INSERT INTO lang (code, description)
VALUES ('zh_tw', 'Chinese Traditional');
COMMIT;

